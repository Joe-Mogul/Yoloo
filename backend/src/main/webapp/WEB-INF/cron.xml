<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
  <cron>
    <url>/crons/resetDailyCron</url>
    <description>Resets daily points for every day.</description>
    <schedule>every 24 hours</schedule>
  </cron>

  <cron>
    <url>/crons/updateGroupSubscriberCountCron</url>
    <description>Updates group subscriber count.</description>
    <schedule>every 2 hours</schedule>
  </cron>

  <cron>
    <url>/crons/deleteOrphanFeedKeysCron</url>
    <description>Deletes orphan feed keys.</description>
    <schedule>every 48 hours</schedule>
  </cron>
</cronentries>